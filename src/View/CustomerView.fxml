<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="555.0" prefWidth="1087.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.CustomerController">
    <children>
        <TableView fx:id="customerTableview" layoutX="16.0" layoutY="6.0" onMouseClicked="#selectCustomerClick" prefHeight="365.0" prefWidth="1051.0">
            <columns>
                <TableColumn fx:id="customerIDColumn" prefWidth="75.0" text="%Customer_ID" />
                <TableColumn fx:id="nameColumn" prefWidth="99.0" text="%Name" />
                <TableColumn fx:id="addressColumn" prefWidth="97.0" text="%Address" />
                <TableColumn fx:id="postalColumn" prefWidth="104.0" text="%Postal_Code" />
                <TableColumn fx:id="phoneColumn" prefWidth="130.0" text="%Phone" />
                <TableColumn fx:id="createdDateColumn" prefWidth="139.0" text="%Created_Date" />
                <TableColumn fx:id="createdByColumn" prefWidth="100.0" text="%Created_By" />
                <TableColumn fx:id="lastUpdateColumn" prefWidth="118.0" text="%Last_Update" />
                <TableColumn fx:id="lastUpdateByColumn" prefWidth="94.0" text="%Last_Update_By" />
                <TableColumn fx:id="divisionIDColumn" prefWidth="94.0" text="%Division_ID" />
            </columns>
        </TableView>
        <Button fx:id="addCustomer" layoutX="693.0" layoutY="378.0" mnemonicParsing="false" onAction="#addCustomerAction" prefHeight="25.0" prefWidth="115.0" text="%Add" />
        <Button fx:id="updateCustomer" layoutX="824.0" layoutY="378.0" mnemonicParsing="false" onAction="#updateCustomerAction" prefHeight="25.0" prefWidth="115.0" text="%Update" />
        <Button fx:id="deleteCustomer" layoutX="958.0" layoutY="378.0" mnemonicParsing="false" onAction="#deleteCustomerAction" prefHeight="25.0" prefWidth="115.0" text="%Delete" />
        <TextField fx:id="nameText" layoutX="129.0" layoutY="378.0" />
        <TextField fx:id="addressText" layoutX="129.0" layoutY="414.0" />
        <TextField fx:id="postalCodeText" layoutX="129.0" layoutY="456.0" />
        <TextField fx:id="phoneText" layoutX="383.0" layoutY="456.0" />
      <Button fx:id="mainMenuButton" layoutX="898.0" layoutY="477.0" mnemonicParsing="false" onAction="#mainMenuAction" text="%Main_Menu" />
        <Button fx:id="appointmentSchedule" layoutX="787.0" layoutY="515.0" mnemonicParsing="false" onAction="#appointmentScheduleAction" text="%Appointment_Schedule" />
        <Button fx:id="logout" layoutX="963.0" layoutY="515.0" mnemonicParsing="false" onAction="#logoutAction" prefHeight="25.0" prefWidth="104.0" text="%Logout" />
        <ComboBox fx:id="stateCombo" layoutX="383.0" layoutY="414.0" prefHeight="25.0" prefWidth="161.0" />
        <ComboBox fx:id="countryCombo" layoutX="383.0" layoutY="378.0" prefHeight="25.0" prefWidth="161.0" />
      <Label fx:id="Name" layoutX="49.0" layoutY="382.0" text="%Name" />
      <Label fx:id="Address" layoutX="42.0" layoutY="418.0" text="%Address" />
      <Label fx:id="Postal_Code" layoutX="16.0" layoutY="460.0" text="%Postal_Code" />
      <Label fx:id="Country" layoutX="297.0" layoutY="382.0" text="%Country" />
      <Label fx:id="State" layoutX="312.0" layoutY="418.0" text="%State" />
      <Label fx:id="Phone" layoutX="309.0" layoutY="460.0" text="%Phone" />
    </children>
</AnchorPane>
